{
  "name": "@park-ui/panda-preset",
  "version": "0.0.1",
  "dependencies": {
    "@ark-ui/anatomy": "3.5.0",
    "change-case": "^5.4.4",
    "effect": "3.10.19"
  },
  "devDependencies": {
    "@park-ui/eslint-config": "workspace:*",
    "@park-ui/prettier": "workspace:*",
    "@pandacss/dev": "0.53.4",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "prettier": "^3.0.0",
    "tsup": "8.4.0",
    "type-fest": "^4.37.0",
    "typescript": "5.8.3"
  },
  "eslintConfig": {
    "extends": ["@park-ui/eslint-config"]
  },
  "exports": {
    ".": {
      "import": {
        "types": "./src/index.ts",
        "default": "./src/index.ts"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./src/utils/index.ts",
        "default": "./src/utils/index.ts"
      },
      "require": {
        "types": "./dist/utils/index.d.cts",
        "default": "./dist/utils/index.cjs"
      }
    },
    "./colors/*": {
      "import": {
        "types": "./src/colors/*.ts",
        "default": "./src/colors/*.ts"
      },
      "require": {
        "types": "./dist/colors/*.d.cts",
        "default": "./dist/colors/*.cjs"
      }
    }
  },
  "files": ["dist"],
  "main": "src/index.ts",
  "module": "src/index.ts",
  "peerDependencies": {
    "@pandacss/dev": "0.53.4"
  },
  "prettier": "@park-ui/prettier",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "pnpm prettier src package.json --write",
    "lint": "pnpm eslint src --fix --ext ts,tsx --max-warnings 0 --report-unused-disable-directives",
    "typecheck": "tsc"
  },
  "tsup": {
    "entry": ["src/index.ts", "src/colors/*.ts", "src/utils/index.ts"],
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "format": ["esm", "cjs"]
  },
  "type": "module",
  "types": "src/index.ts"
}
